						<TextView
							android:id="@+id/lblChiSqCor"
							android:layout_width="1dp"
							android:layout_weight="0.33"
							android:layout_height="wrap_content"
							android:text="..."
							android:textStyle="bold"
							android:gravity="right"
						/>
						<TextView
							android:id="@+id/lblPCor"
							android:layout_width="1dp"
							android:layout_weight="0.33"
							android:layout_height="wrap_content"
							android:text="..."
							android:textStyle="bold"
							android:gravity="right"
						/>
					</LinearLayout>
					<LinearLayout
						android:layout_width="match_parent"
        				android:layout_height="wrap_content"
        				android:weightSum="1"
        				android:layout_marginTop="10dip"
        				android:orientation="horizontal">
        				<TextView
							android:layout_width="1dp"
							android:layout_weight="0.34"
							android:layout_height="wrap_content"
							android:text=""
							android:textColor="#42638c"
		    				android:textStyle="bold"
		    				android:textSize="14sp"
						/>
						<TextView
							android:layout_width="1dp"
							android:layout_weight="0.33"
							android:layout_height="wrap_content"
							android:text="1-Tailed P"
							android:textColor="#42638c"
		    				android:textStyle="bold"
		    				android:textSize="14sp"
							android:gravity="right"
						/>
						<TextView
							android:layout_width="1dp"
							android:layout_weight="0.33"
							android:layout_height="wrap_content"
							android:text="2-Tailed P"
							android:textColor="#42638c"
		    				android:textStyle="bold"
		    				android:textSize="14sp"
							android:gravity="right"
						/>
					</LinearLayout>
					<LinearLayout
						android:layout_width="match_parent"
        				android:layout_height="wrap_content"
        				android:weightSum="1"
        				android:orientation="horizontal">
						<TextView
							android:layout_width="1dp"
							android:layout_weight="0.34"
							android:layout_height="wrap_content"
							android:text="Fisher Exact:"
							android:textColor="#42638c"
				    		android:textStyle="bold"
				    		android:textSize="14sp"
						/>
						<TextView
							android:id="@+id/lblFisher1T"
							android:layout_width="1dp"
							android:layout_weight="0.33"
							android:layout_height="wrap_content"
							android:text="..."
							android:textStyle="bold"
							android:gravity="right"
						/>
						<TextView
							android:id="@+id/lblFisher2T"
							android:layout_width="1dp"
							android:layout_weight="0.33"
							android:layout_height="wrap_content"
							android:text="..."
							android:textStyle="bold"
							android:gravity="right"
						/>
					</LinearLayout>
					<TextView
						android:id="@+id/disclaimer1"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:text=""
						android:textStyle="bold"
						android:gravity="left"
					/>					
					<TextView
						android:id="@+id/disclaimer2"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:text=""
						android:textStyle="bold"
						android:gravity="left"
					/>					
